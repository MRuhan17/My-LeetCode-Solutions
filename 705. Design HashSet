class MyHashSet {
    private boolean[] set;
    private static final int MAX_SIZE = 1000001;

    public MyHashSet() {
        set = new boolean[MAX_SIZE];
    }
    
    public void add(int key) {
        set[key] = true;
    }
    
    public void remove(int key) {
        set[key] = false;
    }
    
    public boolean contains(int key) {
        return set[key];
    }
}
